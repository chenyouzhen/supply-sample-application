<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity AssemblyLine.
    -->
    <changeSet id="20200226140800-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="link_id"
                                 baseTableName="assembly_line"
                                 constraintName="fk_assembly_line_link_id"
                                 referencedColumnNames="id"
                                 referencedTableName="link"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="assembly_line"
                                 constraintName="fk_assembly_line_product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

    </changeSet>
</databaseChangeLog>
